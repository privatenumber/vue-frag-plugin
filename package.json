{
	"name": "vue-frag-plugin",
	"version": "0.0.0-semantic-release",
	"description": "asdf",
	"keywords": [],
	"license": "MIT",
	"repository": "privatenumber/vue-frag-plugin",
	"funding": "https://github.com/privatenumber/vue-frag-plugin?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"require": "./dist/index.js"
		},
		"./loader": {
			"require": "./dist/loader.js"
		},
		"./install-vue-frag": {
			"import": "./dist/install-vue-frag.js"
		}
	},
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "rm -rf dist && rollup -c rollup.config.js",
		"test": "jest",
		"lint": "eslint ."
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint",
			"jest --bail --findRelatedTests"
		]
	},
	"peerDependencies": {
		"vue-frag": "*"
	},
	"dependencies": {
		"@rollup/pluginutils": "^4.1.1",
		"@vue/compiler-dom": "^3.2.20",
		"magic-string": "^0.25.7"
	},
	"devDependencies": {
		"@pvtnbr/eslint-config": "^0.4.1",
		"@rollup/plugin-alias": "^3.1.8",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.6",
		"aggregate-error": "^4.0.0",
		"es-jest": "^1.4.1",
		"eslint": "^8.1.0",
		"esno": "^0.10.1",
		"fs-require": "^1.4.0",
		"husky": "^4.3.8",
		"jest": "^27.3.1",
		"memfs": "^3.3.0",
		"lint-staged": "^11.1.2",
		"outdent": "^0.8.0",
		"rollup": "^2.59.0",
		"rollup-plugin-dts": "^4.0.0",
		"rollup-plugin-esbuild": "^4.6.0",
		"rollup-plugin-vue": "^5.1.9",
		"typescript": "^4.4.4",
		"unionfs": "^4.4.0",
		"vue": "^2.6.14",
		"vue-ast-utils": "^1.0.1",
		"vue-frag": "^1.3.1",
		"vue-loader": "^15.9.8",
		"vue-template-compiler": "^2.6.14",
		"webpack": "^5.61.0",
		"webpack-test-utils": "^1.0.0"
	},
	"eslintConfig": {
		"extends": "@pvtnbr",
		"ignorePatterns": [
			"fixtures",
			"dist"
		],
		"overrides": [
			{
				"files": "**/examples/**",
				"rules": {
					"import/no-unresolved": [
						2,
						{
							"ignore": [
								"vue-frag-plugin"
							]
						}
					]
				}
			}
		]
	}
}
